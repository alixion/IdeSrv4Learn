@using Microsoft.AspNetCore.Authentication

@{
    ViewBag.Title = "This is some secure page";
    Layout = "_Layout";
}

<h1>Secure page</h1>


<hr />
<h2>Claims</h2>

<dl>
    @foreach (var claim in User.Claims)
    {
        <dt>@claim.Type</dt>
        <dd>@claim.Value</dd>
    }
</dl>

<h2>Properties</h2>

<dl>
    @foreach (var prop in (await Context.AuthenticateAsync()).Properties.Items)
    {
        <dt>@prop.Key</dt>
        <dd>@prop.Value</dd>
    }
</dl>